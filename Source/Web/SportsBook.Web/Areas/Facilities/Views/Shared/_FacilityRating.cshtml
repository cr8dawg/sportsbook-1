@model SportsBook.Web.Areas.Facilities.ViewModels.Ratings.FacilityRatingViewModel
<div style="padding-top:5px;padding-bottom:0;padding-left:0;padding-right:0">
    @if (@Model != null)
    {
        <input style="width:25px;padding-right:0;padding-left:8px;padding-top:0px;padding-bottom:0;border-radius:5px" data-id="@this.ViewBag.facilityId" class="user-rating" type="number" placeholder="@Model.RatingValue" />

    }
    else
    {
        <input style="width:25px;padding-right:0;padding-left:8px;padding-top:0px;padding-bottom:0;border-radius:5px" data-id="@this.ViewBag.facilityId" class="user-rating" type="number" placeholder="-" />
    }
</div>















